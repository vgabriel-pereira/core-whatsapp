generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(uuid())
  username String @unique
  password String
}

model Contact {
  id        Int      @id @default(autoincrement())
  name      String
  phone     String @unique
  status    String
  createdAt DateTime @default(now())
}
